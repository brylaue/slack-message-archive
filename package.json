{
  "name": "slack-message-viewer",
  "version": "1.0.0",
  "description": "A web application to view Slack messages older than 90 days",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "install-server": "npm install",
    "install-all": "npm run install-server && npm run install-client"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "axios": "^1.6.0",
    "slack": "^6.8.1",
    "express-session": "^1.17.3",
    "helmet": "^7.1.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": ["slack", "messages", "history", "web-app"],
  "author": "Your Name",
  "license": "MIT"
}